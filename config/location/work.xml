    <work id="garbage" zoneId="work" loadingContent="street,yard,landscape" music="Job2">
        <icon name="GarbageWorkIcon" title="#streetLocationTitle" />
        <session length="30" mode="interval" />
        <scenarios mode="random">
            <scenario id="gb1" tmxmap="street/townsquare_work" type="pick">
                <loot>
                    <item id="1" typeId="gbTrash" minCount="18" maxCount="22" />
                    <item id="2" typeId="gbTrashEnrg" minCount="3" maxCount="5" />
					
					<item id="3" typeId="gbWater" minCount="1" maxCount="1" eventType="service" />
					<item id="4" typeId="gbClay" minCount="1" maxCount="1" eventType="service" />
					<item id="5" typeId="gbLikeCoin" minCount="1" maxCount="1" eventType="trash" />
					<item id="6" typeId="gbFlower" minCount="1" maxCount="1" eventType="service" />
					<item id="7" typeId="gbPeaceOfWood" minCount="1" maxCount="1" eventType="service" />
					<item id="8" typeId="gbPhotograph" minCount="1" maxCount="1" eventType="trash" />
                </loot>
            </scenario>
            <scenario id="gb2" tmxmap="school/science_class_work" type="pick">
                <loot>
                    <item id="1" typeId="gbTrash" minCount="15" maxCount="18" />
                    <item id="2" typeId="gbTrashEnrg" minCount="3" maxCount="5" />

					<item id="3" typeId="gbFeather" minCount="1" maxCount="1" eventType="pillow" />
