					<item id="5" typeId="gbThread" minCount="1" maxCount="1" eventType="pillow" />
					<item id="6" typeId="gbPearl" minCount="1" maxCount="1" eventType="beachRace" />
					<item id="7" typeId="gbShell" minCount="1" maxCount="1" eventType="beachRace" />
					<item id="8" typeId="gbSwordHilt" minCount="1" maxCount="1" eventType="trash" />
                </loot>
            </scenario>
        </scenarios>
    </work>

